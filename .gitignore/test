state("minit")
{
	int reset : "minit.exe", 0x161898, 0x0;
}

init
{
    Action resetVars = () =>
    {
        vars.reset = 4;
    };
    vars.resetVars = resetVars;
    vars.resetVars();
}

split
{
	if (current.reset != old.reset)
	{
		if(current.reset == 5 || current.reset == 7 || current.reset == 9)
			return false;
		else
			return true;
	}
}
